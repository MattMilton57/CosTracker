

<h1>Productions in system</h1>
<ul>
  <% @productions.each do |p| %>
    <li>
      <%= link_to p.title, p %>
      <% theatre = @theatres.find_by id: (p.theatre_id) %>
      <%= theatre.name %>
      <%=link_to "remove", production_path(p), :method=>'destroy'%>
    </li>
    <br>
  <% end %>
</ul>

